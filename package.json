  "scripts": {
    "start": "ng serve",
    "build:lib": "ng-packagr -p src/lib/package.json && npm run copyFilesToDist",
    "build:demo": "ng build --prod",
    "build:all": "npm run build:lib && npm run build:demo",
    "publish:demo": "firebase deploy",
    "publish:lib": "npm publish dist/ngx-hideable-header",
    "publish:all": "npm run publish:lib && npm run publish:demo",
    "copyFilesToDist": "copy LICENSE dist\\ngx-hideable-header && copy README.md dist\\ngx-hideable-header",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tslint": "tslint --project ./",
    "tslint:fix": "tslint --project ./ --fix",
    "prettier": "prettier --parser typescript --write \"./src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahaw/ngx-hideable-header.git"
  },
  "dependencies": {
    "@angular/common": "^6.1.3",
    "@angular/core": "^6.1.3",
    "@angular/platform-browser": "^6.1.3",
    "@angular/platform-browser-dynamic": "^6.1.3",
    "core-js": "^2.5.7",
    "ngx-hideable-header": "0.0.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
